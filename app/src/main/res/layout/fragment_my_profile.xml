                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    app:hintEnabled="true"
                    app:hintTextAppearance="@style/editColor"
                    app:layout_constraintEnd_toEndOf="@+id/textInputLayout109"
                    app:layout_constraintStart_toStartOf="@+id/textInputLayout109"
                    app:layout_constraintTop_toBottomOf="@+id/textInputLayout109">

                    <com.google.android.material.textfield.TextInputEditText
