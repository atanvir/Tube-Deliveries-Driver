                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/clPhonePicker" />

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/textInputLayout109"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    app:hintEnabled="true"
                    app:hintTextAppearance="@style/editColor"
                    app:layout_constraintEnd_toEndOf="@+id/clPhonePicker"
                    app:layout_constraintStart_toStartOf="@+id/clPhonePicker"
                    app:layout_constraintTop_toBottomOf="@+id/view3">

                    <com.google.android.material.textfield.TextInputEditText
