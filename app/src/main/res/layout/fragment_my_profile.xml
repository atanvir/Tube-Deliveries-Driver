                        android:drawableRight="@drawable/down_arrow"
                        android:focusable="false"
                        android:hint="Select State"
                        android:paddingTop="5dp"
                        android:textColor="@android:color/black"
                        android:textSize="15dp" />
                </com.google.android.material.textfield.TextInputLayout>


                <Spinner
                    android:id="@+id/spnState"
                    android:layout_width="0dp"
                    android:layout_height="10dp"
                    android:background="@drawable/drawable_white"
                    app:layout_constraintEnd_toEndOf="@+id/textInputLayout109"
                    app:layout_constraintStart_toStartOf="@+id/textInputLayout109"
                    app:layout_constraintTop_toBottomOf="@+id/textInputLayout109" />

                <View
                    android:id="@+id/view169"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginTop="8dp"
                    android:background="@color/light_grey"
                    app:layout_constraintEnd_toEndOf="@+id/textInputLayout109"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/textInputLayout109"
                    app:layout_constraintTop_toBottomOf="@+id/textInputLayout109" />

                <com.google.android.material.textfield.TextInputLayout
